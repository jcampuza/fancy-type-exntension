{
  "manifest_version": 2,
  "name": "fancy-type",
  "homepage_url": "http://localhost/",
  "description": "A Vue Browser Extension",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "permissions": ["<all_urls>", "*://*/*"],
  "browser_action": {
    "default_popup": "popup/popup.html",
    "default_title": "fancy-type",
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    }
  }
}
